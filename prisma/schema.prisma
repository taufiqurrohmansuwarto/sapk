generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["filterJson", "interactiveTransactions"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  custom_id              String                   @id
  username               String?
  image                  String?
  id                     String?
  from                   String?
  role                   String?
  group                  String?
  employee_number        String?
  birthdate              DateTime?                @db.Date
  last_login             DateTime?                @db.Timestamp()
  email                  String?
  organization           String?
  request_perbaikan_nama request_perbaikan_nama[]
}

model request_perbaikan_nama {
  id             String  @id @default(cuid())
  nip            String?
  nama_master    String?
  nama_sapk      String?
  pembetulan     String?
  diusulkan_oleh String?

  user       users?    @relation(fields: [diusulkan_oleh], references: [custom_id])
  created_at DateTime? @default(now()) @db.Timestamp()
}

model data_import {
  nip        String  @id
  pegawai_id String?
  unor_id    String?
  jfu_id     String?
  jft_id     String?
  no_sk      String?
  tgl_sk     String?
  operator   String?
  nama       String?
  created_at DateTime? @default(now()) @db.Timestamp()
}
